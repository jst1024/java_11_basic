package org.kh.java;

public class Variable4 {
	public static void main(String[] args) {
		String name = "김기태";
		short age = 44;
		float height = 173.6f;
		double weight = 71.9;
		char init = 'k';
		
		System.out.println("이름 : \""+name+"\"");
		
		/* 이스케이프 문자
		 \n : 줄바꿈
		 \t : 탭
		 \\ : \를 출력
		 \" : "를 출력
		 \' : '를 출력
		 */
	}
}
