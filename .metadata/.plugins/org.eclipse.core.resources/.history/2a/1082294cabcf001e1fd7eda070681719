package org.kh.java;

public class Variable5 {
	public static void main(String[] args) {
		char ch1 = 'H';
		char ch2 = 65;
		char ch3 = 97;
		char ch4 = '\u2665';	/* u로 시작하면 유니코드 문자를 의미함 */
		//char ch5 = -65;  char 타입에 정수 입력시 해당 코드 넘버의 문자가 저장되므로 코드 넘버가 음수일 수는 없으므로 오류
		int ch6 = 121;
		System.out.println("ch1 : "+ch1);
		System.out.println("ch2 : "+ch2);
		System.out.println("ch3 : "+ch3);
		System.out.println("ch4 : "+ch4);
		System.out.println("ch6 : "+(char)ch6);
		System.out.println("코드 넘버");
		System.out.println("ch1 : "+(int)ch1);
		System.out.println("ch2 : "+(int)ch2);
		System.out.println("ch3 : "+(int)ch3);
		System.out.println("ch4 : "+(int)ch4);
		System.out.println("ch6 : "+ch6);
	}
}